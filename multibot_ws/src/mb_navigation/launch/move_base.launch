<?xml version="1.0"?>
<launch>
<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
	<param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS"/>
	<param name="base_global_planner" value="global_planner/GlobalPlanner"/>
	<rosparam file="$(find mb_navigation)/cfg/costmap_common_params.yaml" command="load" ns="global_costmap" />
	<rosparam file="$(find mb_navigation)/cfg/costmap_common_params.yaml" command="load" ns="local_costmap" />
	<rosparam file="$(find mb_navigation)/cfg/global_costmap_params.yaml" command="load" />
	<rosparam file="$(find mb_navigation)/cfg/local_costmap_params.yaml" command="load" />
	<rosparam file="$(find mb_navigation)/cfg/global_planner_params.yaml" command="load" />
	<rosparam file="$(find mb_navigation)/cfg/teb_local_planner_params.yaml" command="load" />
	<rosparam file="$(find mb_navigation)/cfg/costmap_converter_params.yaml" command="load" />
	<rosparam file="$(find mb_navigation)/cfg/move_base_params.yaml" command="load"/>
	<remap from="map" to="/map" />
</node>
</launch>
